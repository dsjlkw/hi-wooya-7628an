
config system
	option hostname 'OpenWrt'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '5'
	option zonename 'Asia/Shanghai'
	option timezone 'CST-8'

config timeserver 'ntp'
	list server 'ntp1.aliyun.com'
	list server 'ntp2.aliyun.com'
	list server 'ntp3.aliyun.com'
	list server 'ntp4.aliyun.com'

config led 'led_lan1'
	option name 'lan1'
	option sysfs 'green:lan1'
	option port_mask '0x2'
	option trigger 'netdev'
	option dev 'eth0.1'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led 'led_wan'
	option name 'wan'
	option sysfs 'green:wan'
	option port_mask '0x01'
	option trigger 'netdev'
	option dev 'eth0.2'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led 'led_wifi_led'
	option name 'wifi'
	option sysfs 'green:wlan'
	option trigger 'netdev'
	option dev 'wlan0'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

